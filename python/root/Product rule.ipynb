   "source": [
    "\n",
    "from sympy import init_printing\n",
    "init_printing(use_latex = True)\n",
    "\n",
    "from sympy import *\n",
    "\n",
    "x,y,z,t,a,b,c,d= symbols(\"x y z t a b c d\")\n",
    "\n",
    "#insert the two functions inside the integral here a*b\n",
    "a = x\n",
    "b = exp(x*y)\n",
    "var = y\n",
    "\n",
    "prodrule = diff(a,var)*b+a*diff(b,var)\n",
    "\n",
    "display(prodrule)\n"
